{
  "name": "appetizer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@redux-beacon/google-analytics": "^1.0.2",
    "firebase": "^5.2.0",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-cookie": "^2.2.0",
    "react-dom": "^16.4.1",
    "react-ga": "^2.5.3",
    "react-hubspot-form": "^1.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-promise": "^0.6.0"
  },
  "scripts": {
    "start": "concurrently --kill-others \"react-scripts start\" \"npm run watch-css\"",
    "build": "npm run build-css && react-scripts build",
    "deploy": "npm run build && firebase deploy",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "less-watch-compiler --run-once src/style/ ./",
    "watch-css": "npm run build-css && less-watch-compiler src/style/ ./"
  },
  "devDependencies": {
    "less-watch-compiler": "^1.11.3"
  }
}
